            <div class="my-intro">
                <div class="slideshow-text-div">

                    <span class="my-text-white font-md"><%= slideShow.web.intro %></span><br>
                </div>
                <div class="slideshow-text-div name-div">

                    <span class="my-name my-text-white font-xl b"><%= slideShow.web.name %></span>
                </div>
                <div class="slideshow-text-div">

                    <p class="my-text-white font-sm"><%= slideShow.web.text %></p>
                </div>
            </div>
        </div>
