                <div class="slideshow-text-div">

                    <span class="my-text-white font-md"><%= slideShow.my.intro %></span><br>
                </div>
                <div class="slideshow-text-div name-div">

                    <span class="my-name my-text-white font-xl b"><%= slideShow.my.name %></span>
                </div>
                <div class="slideshow-text-div">

