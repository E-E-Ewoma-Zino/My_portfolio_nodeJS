                    <span class="my-text-white font-md"><%= slideShow.flutter.intro %></span><br>
                </div>
                <div class="slideshow-text-div name-div">

                    <span class="my-name my-text-white font-xl b"><%= slideShow.flutter.name %></span>
                </div>
                <div class="slideshow-text-div">

                    <p class="my-text-white font-sm"><%= slideShow.flutter.text %></p>
                </div>
            </div>
        </div>
        <!-- image and text -->
        <div class="carousel-item my-fade">
            <div class="slideshow-img-div">
                <img src="/images/images_for_index/Ewoma_1.png" alt="My Image" srcset="">
            </div>
            <img src="https://images.unsplash.com/photo-1496964295991-c4a746bdeb8c?ixlib=rb-1.2.1&ixid=MXwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHw%3D&auto=format&fit=crop&w=1405&q=80"
